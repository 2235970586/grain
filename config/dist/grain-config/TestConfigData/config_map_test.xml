<?xml version="1.0" encoding="UTF-8" ?>
<root>
	<!--模板类型的配置文件-->
	<Configs>
		<Config>
			<!--文件名-->
			<path>test.txt</path>
			<!--映射类名-->
			<class>org.grain.config.TestTemplate</class>
		</Config>
	</Configs>
	<!--json类型的配置文件-->
	<JsonConfigs>
		<Config>
			<!--文件名-->
			<path>config.json</path>
			<!--通过字符串获取-->
			<class>config</class>
		</Config>
	</JsonConfigs>
</root>